<p id="notice"><%= notice %></p>

<h1>Ochanges</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Team</th>
      <th>Account</th>
      <th>Supplier</th>
      <th>Po number</th>
      <th>Apr</th>
      <th>May</th>
      <th>Jun</th>
      <th>Jul</th>
      <th>Aug</th>
      <th>Sep</th>
      <th>Oct</th>
      <th>Nov</th>
      <th>Dec</th>
      <th>Jan</th>
      <th>Feb</th>
      <th>Mar</th>
      <th>Other</th>
      <th>Bmo approved</th>
      <th>Central approved</th>
      <th>Central declined</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ochanges.each do |ochange| %>
      <tr>
        <td><%= ochange.description %></td>
        <td><%= ochange.team_id %></td>
        <td><%= ochange.account_id %></td>
        <td><%= ochange.supplier %></td>
        <td><%= ochange.po_number %></td>
        <td><%= ochange.apr %></td>
        <td><%= ochange.may %></td>
        <td><%= ochange.jun %></td>
        <td><%= ochange.jul %></td>
        <td><%= ochange.aug %></td>
        <td><%= ochange.sep %></td>
        <td><%= ochange.oct %></td>
        <td><%= ochange.nov %></td>
        <td><%= ochange.dec %></td>
        <td><%= ochange.jan %></td>
        <td><%= ochange.feb %></td>
        <td><%= ochange.mar %></td>
        <td><%= ochange.other_id %></td>
        <td><%= ochange.bmo_approved %></td>
        <td><%= ochange.central_approved %></td>
        <td><%= ochange.central_declined %></td>
        <td><%= link_to 'Show', ochange %></td>
        <td><%= link_to 'Edit', edit_ochange_path(ochange) %></td>
        <td><%= link_to 'Destroy', ochange, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ochange', new_ochange_path %>
